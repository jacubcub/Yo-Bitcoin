{
    "name": "Yo Bitcoin",
    "version": "1.0",
    "description": "Turn your scrollbar orange if the word bitcoin is on the page!",
    "content_scripts": [
        {
          "matches": ["*://*/*"],
          "js": ["contentScript.js"]
        }
      ],
    "icons": {
        "16": "images/yo-bitcoin-16x16.png",
        "32": "images/yo-bitcoin-32x32.png",
        "48": "images/yo-bitcoin-48x48.png",
        "128": "images/yo-bitcoin-128x128.png"
    },
    "manifest_version": 2
}